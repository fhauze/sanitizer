{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fhauze/sanitizer.git"
    }
  ],
  "require": {
    "fir2be/sanitizer": "dev-main"
  }
}
{
    "name": "fir2be/sanitizer",
    "description": "A package for detecting SQL injection and executable files.",
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-magic": "*"
    },
    "autoload": {
        "psr-4": {
            "fir2be\\Sanitizer\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
